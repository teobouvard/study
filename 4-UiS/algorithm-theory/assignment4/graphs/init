digraph{
	s [pos="0,0!"]
    A [pos="1.5,1.5!"]
    B [pos="4,1.5!"]
    C [pos="1.5,-1.5!"]
    D [pos="4,-1.5!"]
    t [pos="5.5,0!"]

	s -> A [label="0/16"]
	s -> C [label="0/13    "]

	A:sw -> C:nw [label="0/10  "]
	A -> B [label="0/12"]

	C:ne -> A:se [label="0/4"]
	C -> D [label="0/14"]

	B -> C [label="0/9"]
	B -> t [label="0/20    "]

	D -> B [label="0/7"]
	D -> t [label="0/4"]
}